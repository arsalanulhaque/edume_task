{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["plugin:jest/recommended", "airbnb-base", "prettier"],
  "plugins": ["jest"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "jest/expect-expect": [
      "error",
      {
        "assertFunctionNames": ["expect", "request.*.expect"]
      }
    ]
  }
}
